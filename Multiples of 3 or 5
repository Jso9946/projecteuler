#include <iostream>
#include <string>
using namespace std;

int main() {
int x = 3;
int sumofmultiplesof3 = 0;

for (int i = 0; i < 1000; i++) {

if (i % x == 0) {

sumofmultiplesof3 += i;
}
}
int y = 5;
int sumofmultiplesof5 = 0;

for (int j = 0; j < 1000; j++) {

if (j % y ==0) {

sumofmultiplesof5 += j;
}
}

int sum = sumofmultiplesof3 + sumofmultiplesof5;
cout << sum;

return 0;
}

